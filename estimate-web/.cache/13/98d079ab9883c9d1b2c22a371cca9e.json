e":92,"column":6}},{"generated":{"line":173,"column":16},"source":"components/login/Login.js","original":{"line":70,"column":4}},{"generated":{"line":173,"column":17},"source":"components/login/Login.js","original":{"line":69,"column":2}},{"generated":{"line":174,"column":0},"source":"components/login/Login.js","original":{"line":105,"column":1}},{"generated":{"line":174,"column":1},"source":"components/login/Login.js","original":{"line":47,"column":0}},{"name":"Login","generated":{"line":176,"column":15},"source":"components/login/Login.js","original":{"line":107,"column":15}}],"sources":{"components/login/Login.js":"import { useMutation } from \"@apollo/react-hooks\";\nimport gql from \"graphql-tag\";\nimport React from \"react\";\nimport styled from \"styled-components\";\nimport { useForm } from \"react-hook-form\";\nimport { useDispatch } from \"react-redux\";\nimport { get } from \"lodash\";\n\nimport { setSession } from \"#root/store/ducks/session\";\nimport TextInput from \"#root/components/shared/TextInput\";\n\nconst Label = styled.label`\n  display: block;\n\n  :not(:first-child) {\n    margin-top: 0.75rem;\n  }\n`;\n\nconst OrSignUp = styled.span`\n  font-size: 0.9rem;\n`;\n\nconst LabelText = styled.strong`\ndisplay: block\nfont-size: .9rem;\nmargin-bottom: .25rem;\n`;\n\nconst LoginButton = styled.button`\n  display: inline-block;\n  margin-top: 0.5rem;\n`;\n\nconst createUserSessionMutation = gql`\n  mutation($email: String!, $password: String!) {\n    createUserSession(email: $email, password: $password) {\n      id\n      user {\n        email\n        id\n      }\n    }\n  }\n`;\n\nconst Login = () => {\n  const [createUserSession] = useMutation(\n    createUserSessionMutation\n  );\n  const dispatch = useDispatch();\n  const {\n    formState: { isSubmitting },\n    handleSubmit,\n    register,\n  } = useForm();\n\n  const onSubmit = handleSubmit(\n    async ({ email, password }) => {\n      const result = await createUserSession({\n        variables: { email, password },\n      });\n      if (get(result, \"data.createUserSession.id\")) {\n        dispatch(setSession(result.data.createUserSession));\n      }\n    }\n  );\n\n  return (\n    <form onSubmit={onSubmit}>\n      <Label>\n        <LabelText>Email</LabelText>\n        <TextInput\n          disabled={isSubmitting}\n          name=\"email\"\n          type=\"email\"\n          ref={register}\n        />\n      </Label>\n      <Label>\n        <LabelText>Password</LabelText>\n        <TextInput\n          disabled={isSubmitting}\n          name=\"password\"\n          type=\"password\"\n          ref={register}\n        />\n      </Label>\n      <LoginButton type=\"submit\" disabled={isSubmitting}>\n        Login\n      </LoginButton>{\" \"}\n      <OrSignUp>\n        or{\" \"}\n        <a\n          href=\"#\"\n          onClick={(evt) => {\n            evt.preventDefault();\n          }}\n        >\n          Sign Up\n        </a>\n      </OrSignUp>\n    </form>\n  );\n};\n\nexport default Login;\n"},"lineCount":null}},"error":null,"hash":"69ddb19c84d8c320bc5a9bca8295a962","cacheData":{"env":{}}}